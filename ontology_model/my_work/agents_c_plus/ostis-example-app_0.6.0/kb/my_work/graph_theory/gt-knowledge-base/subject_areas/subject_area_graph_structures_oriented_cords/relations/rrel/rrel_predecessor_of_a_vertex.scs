rrel_predecessor_of_a_vertex => nrel_idtf: [predecessor of a vertex'](*<-lang_en;;*);;

rrel_predecessor_of_a_vertex <- rrel_key_sc_element:
...(*	
	=> nrel_main_idtf: [Опр.(предок вершины')] (* <- lang_ru;; *);;
	<=nrel_using_constants:
			{
				rrel_vertex
			};;
	<- definition;;
	<= nrel_sc_text_translation: ...(*
						-> [Предок вершины -  для вершины w всякая вершина v, из которой достижима w. ](*<- lang_ru;;*);;
						*);;

	*);;


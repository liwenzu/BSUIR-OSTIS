	...(*
		<- sc_statement;;
		=> nrel_main_idtf: [Утв.(предок вершины')] (* <- lang_ru;; *);;
		<= nrel_sc_text_translation: ...(*
			-> [ V называется непосредственным предком (или предшественником), если существует дуга (v,w). ](*<- lang_ru;;*);;
			*);;
*);;

rrel_predecessor_of_a_vertex <- rrel_key_sc_element:
	...(*
		<- sc_statement;;
		=> nrel_main_idtf: [Утв.(предок вершины')] (* <- lang_ru;; *);;
		<= nrel_sc_text_translation: ...(*
			-> [Непосредственный предок в ордереве часто называется отцом вершины w. ](*<- lang_ru;;*);;
			*);;
*);;

